<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.me">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/yukti"
        android:label="@string/app_name"
        android:roundIcon="@drawable/yukti_round"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">

        <activity android:name=".Orders.Old_Order_List"></activity>

        <activity android:name=".AboutUs" />
        <activity
            android:name=".Orders.Order_History"
            android:label="@string/title_activity_order__history"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".home.Cart"
            android:parentActivityName=".home.Catagories" />

        <activity android:name=".Admin.AdminCategoryActivity" />
        <activity android:name=".Admin.Admin_Add_New_Product_Activity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".login" />

        <activity
            android:name=".Register.profile"
            android:label="@string/title_activity_profile"
            android:parentActivityName=".home.MainActivity" />
        <activity android:name=".Register.Otp" />
        <activity
            android:name=".Register.register"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".home.Catagories"
            android:parentActivityName=".home.MainActivity" /> <!-- splash Activity -->
        <activity android:name=".home.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <!-- main activity -->
        </activity> <!-- <service android:name=".service.MyFirebaseInstanceService"> -->
        <!-- <intent-filter> -->
        <!-- <action android:name="com.google.firebase.MESSAGING_EVENT" /> -->
        <!-- </intent-filter> -->
        <!-- </service> -->
        <activity android:name=".home.MainActivity" />
    </application>

</manifest>